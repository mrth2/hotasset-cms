{
  "kind": "collectionType",
  "collectionName": "assets",
  "info": {
    "name": "asset",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "resources": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "types": {
      "collection": "asset-type"
    },
    "channels": {
      "collection": "asset-channel"
    },
    "can_download": {
      "type": "boolean",
      "default": true
    },
    "category": {
      "model": "category"
    },
    "tags": {
      "collection": "tag"
    },
    "likes": {
      "type": "integer",
      "default": 0
    },
    "upvoters": {
      "via": "upvoted_assets",
      "plugin": "users-permissions",
      "collection": "user",
      "dominant": true
    },
    "author": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
